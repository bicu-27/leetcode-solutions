class Solution:
    def isEven(self, n):
        if n % 2 == 0:
            return True
        else:
            return False 

# Question three 
class Solution:
def seriesSum(self, n : int) -> int:
        n = 1/2*n**2 + 1/2 *n
        return int(round(n))
solution = Solution()
solution.seriesSum(4)
      
